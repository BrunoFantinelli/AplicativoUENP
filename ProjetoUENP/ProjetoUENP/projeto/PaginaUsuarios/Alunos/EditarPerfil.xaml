<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            
             x:Class="ProjetoUENP.projeto.PaginaUsuarios.Alunos.EditarPerfil" Title="Editar Perfil">
    <ContentPage.Content>
        <StackLayout  Padding="30" VerticalOptions="CenterAndExpand">
            <Button Text="Editar Informações" TextColor="White" BackgroundColor="#183883" IsEnabled="True" Clicked="EditarInfo"/>
            <Button Text="Mudar Senha" TextColor="White" BackgroundColor="#183883" IsEnabled="True" Clicked="MudarSenha"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>