<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Alimentação" x:Class="ProjetoUENP.projeto.Funcionalidades.FuncAlimentacao.Alimentacao">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout Padding="30" VerticalOptions="CenterAndExpand">
                <ActivityIndicator x:Name="activityIndicator1" IsRunning="{Binding IsBusy}" Color="#183883" VerticalOptions="StartAndExpand"/>
                <Button Text="CAMPUS CORNÉLIO PROCÓPIO" TextColor="White" BackgroundColor="#183883" FontSize="Medium" Clicked="AlimentacaoCP" IsEnabled="True"/>
                <Button Text="CAMPUS JACAREZINHO" TextColor="White" BackgroundColor="#183883" FontSize="Medium" Clicked="AlimentacaoJacare" IsEnabled="True"/>
                <Button Text="CAMPUS LUIZ MENEGHEL" TextColor="White" BackgroundColor="#183883" FontSize="Medium" Clicked="AlimentacaoCLM" IsEnabled="True"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>